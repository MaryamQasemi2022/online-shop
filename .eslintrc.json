{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
      "linebreak-style": 0,
      "react/function-component-definition":[0, {
        "namedComponents": "arrow-function"
      }],
      "no-unused-vars":1,
      "react/no-unknown-property": [2, { "ignore": ["css"] }],
      "react/prop-types":0,
      "react/jsx-props-no-spreading":0,
      "react/self-closing-comp": [0, {
        "component": true,
        "html": true
      }],
      "import/no-cycle": [0, { "maxDepth": 40 }]
    }
}
